<div id="Bookings" class="container-booking">
  <h1>Activity</h1>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Event Name</th>
        </tr>
      </thead>
      <tbody>
        <% @bookings.each_with_index do |booking, index| %>
          <tr>
            <th scope="row"><%= index + 1 %></th>
            <td><%= booking.event %></td>


          </tr>
        <% end %>
    </tbody>
  </table>
</div>
